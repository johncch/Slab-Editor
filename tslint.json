{
	"extends": [
		"tslint:latest"
	],
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,		
		"curly": true,
		"class-name": true,
		"semicolon": ["always"],
		"triple-equals": true,
        "max-classes-per-file": [
            false
        ],
		"member-access": [
			false
		],
		"member-ordering": [
			false
		]
	}
}